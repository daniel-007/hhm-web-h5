<div class="page-group">
    <div id="page-my-buy-report-result" class="page page-current">
        <nav class="bar bar-tab">
            <div class="my-buy-report-total my-gray-text">实付合计：<span class="my-red-text" v-cloak>￥${reports.OrderTotalAmount}</span> 优惠合计：<span class="my-red-text" v-cloak>￥${reports.PromotionTotalAmount}</span></div>
        </nav>

        <div class="content native-scroll">
            <table class="my-table-fix" width="100%" border="0" cellspacing="0" cellpadding="0">
                <thead>
                <tr>
                    <th width="50%">商品名称</th>
                    <th width="20%">数量</th>
                    <th width="30%">金额</th>
                </tr>
                </thead>
            </table>

            <table class="my-table-buy-report my-gray-text" width="100%" border="0" cellspacing="0" cellpadding="0">
                <tbody>
                <template v-for="item in reports.Items">
                    <tr v-cloak>
                        <td width="50%">${item.ProductName}</td>
                        <td width="20%">${item.Quantity}</td>
                        <td width="30%">￥${item.ProductAmount}</td>
                    </tr>
                </template>
                </tbody>
            </table>
        </div>
    </div>
</div>
<script type='text/javascript' src='//g.alicdn.com/sj/lib/zepto/zepto.min.js' charset='utf-8'></script>
<script type='text/javascript' src='//g.alicdn.com/msui/sm/0.6.2/js/sm.min.js' charset='utf-8'></script>
<script type='text/javascript' src='../js/lib/requirejs.min.js' data-main='../js/controller/user-ctrl.min'></script>
